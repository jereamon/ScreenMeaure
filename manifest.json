{
	"manifest_version": 2,
	"name": "ScreenMeasure",
	"version": "0.01",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"css": ["content.css"],
			"js": ["content.js"]
		}
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "Icon-16x16.png",
		"48": "Icon-48x48.png",
		"128": "Icon-128x128.png"
	}
}
